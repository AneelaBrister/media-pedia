{
    "name": "Media Pedia",
    "version": "1.0",
    "description": "Pedia some Media",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "scripting",
        "activeTab", "declarativeContent", "storage","contextMenus"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }    
}